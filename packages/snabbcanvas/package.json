{
  "name": "@genjer/snabbcanvas",
  "version": "0.0.0",
  "description": "This package exposes a declarive drawing API for snabbdom",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "es/index.js",
  "author": "Syaiful Bahri",
  "license": "MIT",
  "peerDependencies": {
    "snabbdom": ">= 0.7.3 < 1.0"
  },
  "devDependencies": {
    "typescript": "^3.7.2",
    "mocha": "^5.2.0",
    "snabbdom": "^0.7.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^12.12.7",
    "rollup": "^1.27.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.25.2"
  },
  "scripts": {
    "compile": "yarn compile-es && yarn compile-commonjs && yarn compile-example",
    "compile-es": "tsc --outDir es --module es6 --moduleResolution node",
    "compile-commonjs": "tsc --outDir lib",
    "compile-example": "rollup -c"
  }
}
